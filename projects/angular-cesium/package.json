{
  "name": "@auscope/angular-cesium",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Angular v13 library for working with Cesium.",
  "repository": "https://github.com/AuScope/angular-cesium",
  "repository": {
    "type": "git",
    "url": "https://github.com/AuScope/angular-cesium.git",
    "directory": "projects/angular2parse"
  },
  "keywords": [
    "Angular",
    "Cesium",
    "Angular Cesium"
  ],
  "peerDependencies": {
    "@angular/common": "^13.0.0",
    "@angular/core": "^13.0.0",
    "lodash": "^4.17.21",
    "cesium": ">= 1.48.0"
  },
  "dependencies": {
    "@auscope/angular2parse": "^2.0.2",
    "geodesy": "^1.1.1",
    "@types/geodesy": "^1.1.2",
    "json-string-mapper": "^1.0.0",
    "primitive-primitives": "0.0.6",
    "tslib": "^2.3.1"
  },
  "optionalDependencies": {
    "heatmap.js": "^2.0.5"
  },
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy:files": "cp -a ./schematics/ng-add ../../dist/angular-cesium/schematics",
    "copy:collection": "cp ./schematics/collection.json ../../dist/angular-cesium/schematics/collection.json",
    "postbuild": "npm run copy:files && npm run copy:collection"
  },
  "schematics": "./schematics/collection.json"
}
