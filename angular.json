{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-cesium-demo": {
      "root": "projects/demo",
      "sourceRoot": "projects/demo/src",
      "projectType": "application",
      "prefix": "",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/angular-cesium-demo",
            "index": "projects/demo/src/index.html",
            "main": "projects/demo/src/main.ts",
            "polyfills": "projects/demo/src/polyfills.ts",
            "tsConfig": "projects/demo/tsconfig.app.json",
            "assets": [
              "projects/demo/src/favicon.ico",
              "projects/demo/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/cesium/Build/Cesium",
                "output": "./assets/cesium"
              }
            ],
            "styles": [
              "projects/demo/src/styles.css",
              "projects/demo/src/app/utils/loading.css",
              "./node_modules/cesium/Build/Cesium/Widgets/widgets.css"
            ],
            "scripts": [
              "./node_modules/cesium/Build/Cesium/Cesium.js"
            ],
            "allowedCommonJsDependencies": [
              "geodesy",
              "util"
            ],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/demo/src/environments/environment.ts",
                  "with": "projects/demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular-cesium-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-cesium-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-cesium-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/demo/src/test.ts",
            "polyfills": "projects/demo/src/polyfills.ts",
            "tsConfig": "projects/demo/src/tsconfig.spec.json",
            "karmaConfig": "projects/demo/src/karma.conf.js",
            "styles": ["projects/demo/src/styles.css"],
            "scripts": [
              "./node_modules/cesium/Build/Cesium/Cesium.js"
            ],
            "assets": ["projects/demo/src/favicon.ico", "projects/demo/src/assets"]
          }
        }
      }
    },
    "newlib-angular-cesium-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "angular-cesium-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-cesium-demo:serve:production"
            }
          }
        }
      }
    },
    "angular-cesium": {
      "root": "projects/angular-cesium",
      "sourceRoot": "projects/angular-cesium/src",
      "projectType": "library",
      "prefix": "ac",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/angular-cesium/tsconfig.lib.json",
            "project": "projects/angular-cesium/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/angular-cesium/tsconfig.lib.prod.json"
            }
          }
},
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/angular-cesium/src/test.ts",
            "tsConfig": "projects/angular-cesium/tsconfig.spec.json",
            "karmaConfig": "projects/angular-cesium/karma.conf.js"
          }
        }
      }
    },
    "landing-page": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/landing-page",
      "sourceRoot": "projects/landing-page/src",
      "prefix": "",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/landing-page",
            "index": "projects/landing-page/src/index.html",
            "main": "projects/landing-page/src/main.ts",
            "polyfills": "projects/landing-page/src/polyfills.ts",
            "tsConfig": "projects/landing-page/tsconfig.app.json",
            "assets": [
              "projects/landing-page/src/favicon.ico",
              "projects/landing-page/src/assets",
              {
                "glob": "**/*",
                "input": "./node_modules/cesium/Build/Cesium",
                "output": "./assets/cesium"
              }
            ],
            "styles": [
              "projects/landing-page/src/styles.scss",
              "./node_modules/cesium/Build/Cesium/Widgets/widgets.css"
            ],
            "scripts": ["./node_modules/cesium/Build/Cesium/Cesium.js"],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/landing-page/src/environments/environment.ts",
                  "with": "projects/landing-page/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "landing-page:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "landing-page:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "landing-page:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/landing-page/src/test.ts",
            "polyfills": "projects/landing-page/src/polyfills.ts",
            "tsConfig": "projects/landing-page/tsconfig.spec.json",
            "karmaConfig": "projects/landing-page/karma.conf.js",
            "assets": [
              "projects/landing-page/src/favicon.ico",
              "projects/landing-page/src/assets"
            ],
            "styles": [
              "projects/landing-page/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/landing-page/e2e/protractor.conf.js",
            "devServerTarget": "landing-page:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "landing-page:serve:production"
            }
          }
        }
      }
    }
  },
  "defaultProject": "angular-cesium-demo"
}
